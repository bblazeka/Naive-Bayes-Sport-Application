<Application x:Class="HockeyDb.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HockeyDb"
             xmlns:sys="clr-namespace:System;assembly=System.Runtime"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <x:Array x:Key="Seasons" Type="{x:Type sys:Int32}">
            <sys:Int32>20202021</sys:Int32>
            <sys:Int32>20192020</sys:Int32>
            <sys:Int32>20182019</sys:Int32>
            <sys:Int32>20172018</sys:Int32>
            <sys:Int32>20162017</sys:Int32>
        </x:Array>
    </Application.Resources>
</Application>
